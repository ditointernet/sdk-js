{
  "name": "dito",
  "version": "0.1.1",
  "description": "Dito SDK for JavaScript",
  "main": "dist/dito.js",
  "module": "dist/dito.es.js",
  "browser": "dist/dito.min.js",
  "scripts": {
    "format": "pretty-quick --staged --pattern '{lib}/**/*.{js,ts}'",
    "lint": "eslint --fix \"lib/**/*{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ditointernet/sdk-js.git"
  },
  "keywords": [
    "dito",
    "js",
    "javascript",
    "ts",
    "identify",
    "track",
    "typescript"
  ],
  "author": "Dito",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ditointernet/sdk-js/issues"
  },
  "husky": {
    "hooks": {
      "precommit": "npm run format && npm run lint"
    }
  },
  "homepage": "https://github.com/ditointernet/sdk-js#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.1.2",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.3.3"
  }
}
