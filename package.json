{
  "name": "dito",
  "version": "0.1.1",
  "description": "Dito SDK for JavaScript",
  "main": "dist/dito.js",
  "module": "dist/dito.es.js",
  "browser": "dist/dito.min.js",
  "scripts": {
    "format": "pretty-quick --staged --pattern '{lib}/**/*.{js}'",
    "lint": "eslint --fix \"lib/**/*{js}\"",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ditointernet/sdk-js.git"
  },
  "keywords": [
    "dito",
    "js",
    "javascript",
    "identify",
    "track"
  ],
  "author": "Dito",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ditointernet/sdk-js/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "homepage": "https://github.com/ditointernet/sdk-js#readme",
  "devDependencies": {
    "eslint": "^5.14.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.1.2",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "sha1-hex": "^1.0.0",
    "superagent": "^4.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base"
  }
}
